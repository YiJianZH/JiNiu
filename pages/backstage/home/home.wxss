/*左侧栏主盒子*/

#nav_left {
  display: inline-block;
  width: 25%;
  height: 75%;
  background: #f0f0f0;
  position: fixed;
  left: 0px;
  top: 193rpx;
  margin-top: 0px;
  z-index: 10;
  text-align: center;
  box-shadow: inset 0 1px 6px rgba(0, 0, 0, 0.1);
}

/*左侧栏list的item*/
.left_lists {
  margin-bottom: 400rpx;
}

#nav_left .nav_left_items {
  height: 2rem;
  line-height: 2rem;
  padding: 6px 0;
  border-bottom: 1px solid #dedede;
  font-size: 26rpx;
  color: #333;
}

#nav_left .nav_left_items.active {
  background: #fff;
  border-left: 6rpx solid #f90;
  color: #f90;
}
.MenList {
  height: 100%;
}
#wrap,
#wrap_third {
  animation-fill-mode: forwards;
  width: 100%;
  overflow: hidden;
}
#wrap .box,
#wrap_third .box {
  width: 100%;
  background: #fff;
  overflow: auto;
}
.second_lists {
  overflow: auto;
  width: 100%;
  padding: 10rpx 0;
  font-size: 31rpx;
  color: #707070;
}
.third_lists {
  overflow: auto;
  width: 100%;
  padding: 10rpx 0;
  font-size: 30rpx;
  text-indent: 15rpx;
  color: #999;
}
@keyframes slidedown {
  from {
    height: 0;
  }
  to {
    height: 220rpx;
  }
}
@keyframes slideup {
  from {
    height: 220rpx;
  }
  to {
    height: 0;
  }
}
.show {
  display: block;
}
.hide {
  display: none;
}
.open {
  animation: slidedown .2s;
}
.close {
  animation: slideup .2s;
}


/*右侧栏主盒子*/
#nav_right {
  position: fixed;
  top: 310rpx;
  margin-top: 0px;
  right: 0;
  flex: 1;
  width: 75%;
  height: 100%;
  box-sizing: border-box;
  z-index: 0;
  overflow: hidden;
  padding-bottom:100rpx;
}
#nav_right .box {
