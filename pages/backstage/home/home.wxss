}

.ptLiImg image{
  display: block;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50px;
}

.ptBtn{
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  border-top: solid 1px #e6e6e6;
}

.ptBtn button{
  width: 50%;
  height: auto;
  overflow: hidden;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #333;
  border-radius: 0px;
  background-color: transparent;
}

.ptBtn button:last-child{
  border-left: solid 1px #e6e6e6;
  background-color: #f90;
  color: #fff;
}

button::after{
  border-radius: 0px;
  border: none;
}