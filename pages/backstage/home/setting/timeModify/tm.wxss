/* pages/backstage/home/setting/timeModify/tm.wxss */
page{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
}
.tm {
  font-size: 28rpx;
}
.lists {
  padding: 0 30rpx;
}
.each_list {
  padding: 30rpx 0;
  border-bottom: 1px solid #e6e6e6;
  display: flex;
}
.each_list label {
  width: 30%;
}
.each_list .w {
  width: 70%;
  display: flex;
  justify-content: flex-end;
}
.each_list .w image {
  width: 26rpx;
  height: 26rpx;
  margin-left: 20rpx;
}
.mt30 {
  margin-top: 30rpx;
}

button {
  width: 92%;
  line-height: 90rpx;
  margin: 200rpx auto 0;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background: #f90;
  border-radius: 4px;
}