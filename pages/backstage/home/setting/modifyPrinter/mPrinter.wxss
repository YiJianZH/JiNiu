/* pages/backstage/home/setting/modifyPrinter/mPrinter.wxss */
page{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
}
.mPrinter {
  font-size: 28rpx;
}
.lists {
  padding: 0 30rpx;
}
.each_list {
  padding: 20rpx 0;
  border-bottom: 1px solid #e6e6e6;
  display: flex;
}
.each_list label {
  width: 30%;
  line-height: 50rpx;
}
.each_list .w {
  width: 70%;
}
.each_list .w input {
  width: 100%;
  height: 100%;
}
.ttb {
  display: block;
}
.ttb label {
  display: block;
  width: 100%;
  margin-bottom: 20rpx;
}

button {
  width: 92%;
  line-height: 90rpx;
  color: #fff;
  background: #f90;
  text-align: center;
  margin: 200rpx auto 0;
  border-radius: 4px;
  font-size: 28rpx;
}