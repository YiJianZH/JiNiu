/* pages/home/setting/setting.wxss */
page{
  width: 100%;
  height: 100%;
  overflow-x: hidden;
}
.setting {
  font-size: 26rpx;
  color: #333;
}
.lists {
  padding: 0 30rpx;
}
.each_list {
  padding: 30rpx 0;
  border-bottom: 1px solid #e6e6e6;
  display: flex;
  justify-content: space-between;
}
.each_list label {
  width: 30%;
}
.each_list .w {
  width: 70%;
  display: flex;
  justify-content: flex-end;
}
.each_list .w image {
  width:29rpx;
  height:32rpx;
}
.switch label {
  line-height: 67rpx;
}
.time_a, .time_b {
  display: flex;
}
.ttb {
  display: block;
}
.ttb label {
  width: 100%;
}
.ttb .w {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
  width:100%;
}
.time_modify {
  padding: 0 10rpx;
  text-align: center;
  border: 1px solid #f90;
  border-radius: 3px;
}

.color_f90 {
  color: #f90;
}
.color_red {
  color: red;
}
.fz24 {
  font-size: 24rpx;
}

.tabBar {
  background: #fff;
  border-top: 1px solid #f5f5f5;
  position: fixed;
  bottom: 0;
  z-index: 1001;
  width: 100%;
  padding: 5rpx 0 10rpx 0;
  font-size: 24rpx;
  color: #999;
}
.tabBar .con_info {
  display: flex;
  justify-content: space-around;
  width: 80%;
  margin: 0 auto;
}
.tabBar .w image {
  width: 50rpx;
  height: 50rpx;
  margin: 5rpx auto;
  display: block;
}
.tabBar .w text {
  text-align: center;
  display: block;
}