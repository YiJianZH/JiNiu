 /*shop_list.wxss*/
@import "/wxSearch/wxSearch.wxss";
page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.ptTxt{
  overflow: hidden;
  /* display: flex;
  justify-content: space-between;
  align-items: center; */
  margin-top: 5rpx;
}

.ptTxt > text{
  font-size: 28rpx;
  color: #999999;
  text-decoration:line-through;
  float: left;
}

.shop_one{
  width: 100%;
  height: auto;
  overflow: hidden;
}

.ptsea{
  width: 70%;
  height: auto;
  overflow: hidden;
}

.pt{
  width: 100%;
  height:auto;
  overflow: hidden;
  margin-bottom: 20rpx;
}


.input{
  width:100%;
  height:100%;
  background-color:#FFF;
  font-size:26rpx;
  color:#666666;
  text-indent:1em;
  line-height:64rpx;
  margin-left:4%;
  float: left;
}
.SeaIcon{
width:28%;
height:100%;
text-align:center;
margin-right:1em;

}

.SeaIcon icon{
  float: right;
}

.Search{
  width:100%;
  height:30px;
  margin-top:10px;
  background-color: #FFF;
  border-radius: 50rpx;
  display: flex;
}
.shop_one_a{
  width: 95%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
  -moz-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -o-flex-wrap: wrap;
}
.shop_one_aa:nth-of-type(even){
float: right;
}

.shop_one_aa{
  width: 49%;
  height: auto;
  overflow: hidden;
  background-color: #fff;
  margin-top: 20rpx;
  border-radius: 20rpx;
  float:left;
}

.shop_one_b{
  width: 85%;
  height: auto;
  margin: 0 auto;
  overflow: hidden;
  padding:10rpx 0rpx;
  position: relative;
}

.ptImg{
  width: 110rpx;
  height: 110rpx;
  margin: 0 auto;
  position: absolute;
  left:50%;
  top:50%;
  margin-left:-55rpx;
  margin-top: -55rpx;
  z-index: 999;
}

.shop_one_img{
  width:270rpx;
  height: 270rpx;
  display: block;
  margin:10rpx auto 0 auto; 
}
.shop_one_text{
  font-size: 26rpx;
  color: #1a1a1a;
  font-family: "微软雅黑";
  margin-top: 28rpx;
  display: block;
  height: auto;
  max-height: 70rpx;
  
  text-overflow: ellipsis;
  overflow : hidden;
  white-space: nowrap;
display: -webkit-box;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
line-height:34rpx;
}
.shop_one_c{
  width: 100%;
  overflow: hidden;
  position: relative;
  height: auto;

  margin-top: 18rpx;
  
}
.shop_one_d{
  width: auto;
  height: auto;
  overflow: hidden;
  position: relative;
  font-size:30rpx;
  font-weight: 600;
  color: #ff0000;
  font-family: "微软雅黑";
  display:flex;

}
.shop_one_d text{
  font-size: 28rpx;
  color: #999999;
  font-weight: normal;
}
.shop_one_e_img{
  width:44rpx;
  height: 44rpx;
  display: inline-block;
  margin-left: 16rpx;
}
.shop_sc_img{
  width:35rpx;
  height:35rpx;
  display: inline-block;
}
.shop_one_e_img:last-child{
  width:34rpx;
  height:34rpx;
  
}
.shop_img{
  float: right;
  width: 105rpx;
  height: 44rpx;
  /* position: absolute;
  right: 0rpx;
  top:0rpx; */
  flex-direction: row;
  -moz-flex-direction: row;
  -o-flex-direction: row;
  -webkit-flex-direction: row;
}
.Msageclass{
  width: 100%;
  text-align: center;
  margin-top: 20px;
 margin-bottom: 10px;
 font-size: 28rpx;
 color: #9a9a9a;

}

@media only screen and (max-width:320px ) {
	.shop_one_e_img{
  margin-left: 20rpx;
}
.shop_img{
  width: 110rpx;}
}



