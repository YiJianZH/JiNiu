/*shop_money.wxss */
page{
  background-color: #f5f5f5;
  width: 100%;
  height: 100%;
}
.money_a{
  width: 80%;
  height: auto;
  overflow: hidden;
  margin:110rpx auto 0px auto;
}
.money_img{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.money_imga{
  width: 120rpx;
  height: 120rpx;
  display: block;
  overflow: hidden;
  margin: 0 auto;
}
.money_text{
  width: 100%;
  display: block;
  font-size: 32rpx;
  color: #333333;
  text-align:center;
  padding-top:30rpx;
}
.money_b{
   width: 100%;
   height: auto;
   overflow: hidden;
   margin-top:56rpx;
}
.money_b text.money_ba{
  font-size: 30rpx;
  color: #333333;
  line-height: 55rpx;
  display: block;
}
.money_b text.money_ba:last-child text{
  color: #f70a0a;
}
.money_c{
  width: 100%;
  height: auto;
  overflow:hidden;
  margin-top:40rpx;
}
.money_c text{
  display: block;
  font-size: 24rpx;
  color: #999999;
  text-align: left;
  line-height: 40rpx;
}
.money_c text:last-child{
  margin-top:20rpx;
}



