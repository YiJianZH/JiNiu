/*shop_list.wxss*/

.loading{  
 padding:10rpx;
text-align:center;
background-color:#EDECE8;
  font-size:22rpx; 
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  color: #333;
}  
.loadingimg{
  width: 20px;
  height: 20px;
}  
.loading.complete:before{  
  display: none;  
}  


.null{
  width: 100%;
  height: auto;
  position: fixed;
  left: 50%;
  top:38%;
  margin-top: -25%;
  margin-left: -50%;
}

.null image{
  width: 200px;
  height: 170px;
  margin: 0 auto;
  display: block;
}



page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.shop_one{
  width: 100%;
  height: 100%-10px;
  overflow: hidden;

}
.shop_one_a{
  width: 95%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
  -moz-flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -o-flex-wrap: wrap;
}
.shop_one_aa{
  width: 48.5%;
  height: auto;
  overflow: hidden;
  background-color: #fff;
  margin-right: 21rpx;
  margin-top: 20rpx;
  border-radius: 20rpx;
        display: flex;
        justify-content: space-between;
        -webkit-justify-content: space-between;
        -moz-justify-content: space-between;
        -o-justify-content: space-between;

}
.shop_one_aa:nth-of-type(even){
   margin-right: 0rpx;
}
.shop_one_b{
  width: 85%;
  min-height: 430rpx;
  height: 460rpx;
  margin: 0 auto;
  overflow: hidden;
  padding:10rpx 0rpx;
}

.shop_one_img{
  width:270rpx;
  height: 270rpx;
  display: block;
  margin:10rpx auto 0 auto;  
}
.shop_one_text{
  font-size: 24rpx;
  color: #1a1a1a;
  font-family: "微软雅黑";
  margin-top: 28rpx;
  display: block;
  height: 70rpx;
  isplay: -webkit-box;
  text-overflow: ellipsis;
  overflow : hidden;
display: -webkit-box;
-webkit-line-clamp: 2;
-webkit-box-orient: vertical;
line-height:34rpx;
}
.shop_one_c{
  width: 100%;
  overflow: hidden;
  position: relative;
  height: 46rpx;
  line-height: 46rpx;
  margin-top: 18rpx;
  
}
.shop_one_d{
  width: 142rpx;
  height: auto;
  overflow: hidden;
  position: relative;
  font-size: 26rpx;
  color: #ff0000;
  font-family: "微软雅黑";
}
.shop_one_d text{
  font-size: 23rpx;
  color: #999999;
}
.shop_one_e_img{
  width:44rpx;
  height: 44rpx;
  display: inline-block;
  margin-left: 16rpx;
}
.shop_one_e_img:last-child{
  width:34rpx;
  height:34rpx;
  
}
.shop_img{
  width: 105rpx;
  height: 44rpx;
  position: absolute;
  right: 0rpx;
  top:0rpx;
  flex-direction: row;
  -moz-flex-direction: row;
  -o-flex-direction: row;
  -webkit-flex-direction: row;
}


@media only screen and (max-width:320px ) {
	.shop_one_e_img{
  margin-left: 20rpx;
}
.shop_img{
  width: 110rpx;}
}


