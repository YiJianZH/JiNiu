/**选项卡*/
.b1{position:fixed;width:100%;height:100%;top:0px;background:rgba(0,0,0,0.4);overflow: hidden;} 
.tc{
  width: 650rpx;
  height:auto;
  background-color: #fff;
  position: fixed;
  left: 50%;
  top:50%;
  margin-left:-325rpx;
  margin-top: -301rpx; 
  border-radius: 20rpx;
  }
.tc_a{
  width: 100%;
  height: auto;
  border-top-left-radius: 10px;
  border-top-right-radius:10rpx;
  font-size: 32rpx;
  color: #fff;
  line-height: 80rpx;
  background-color: #47b34e;
  text-align: left !important;
 text-indent: 1em;
}
.tc_b{
  width: 90%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.tc_ba{
  font-size: 32rpx;
  color: #333;
  text-align: left !important;
  display: block;
}
.tc_bb{
  width: 90%;
  margin: 0 auto;
  height: auto;
  overflow: hidden;
}
.bb_li{
  width: 200rpx;
  height: 54rpx;
  font-size: 32rpx;
  color: #333;
  text-align: center;
  line-height: 54rpx;
  border-radius: 50rpx;
  border: solid 1px #333;
  float: left;
  margin-right: 19rpx;
  margin-top: 30rpx;
}
.bb_li:nth-child(3){
  margin-right: 0rpx;
}
.tc_input{
  border: none;
  
}


.button{
  width: 80%;
  height: auto;
  font-size: 32rpx;
  color: #fff;
  margin:40rpx auto 0 auto;
}
button{
  background-color: #47b34e !important;
}
button::after{
  border: none;
}

.swiper-tab{  
    width: 100%;  
    border-bottom: 2rpx solid #e6e6e6;  
    text-align: center;  
    line-height: 80rpx;}  
.swiper-tab-list{  font-size: 30rpx;  
    display: inline-block;  
    width: 20%;  
    color: #333;  
}
.on{ color: 5fd03f;  
    border-bottom: 5rpx solid #47b34e;}  
  
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; }  
.swiper-box view{  
    text-align: right;  
}  
.text_pt{
  text-align: center;
  margin-top: 20rpx;
  color: red;
  display: block;
}
.xa{
  width: 48rpx;
  height: 48rpx;
