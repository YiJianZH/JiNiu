/* 弹窗/超出范围 */
.outpop {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.3);
  z-index: 999;
  overflow: hidden;
}
.outpop .pop_w {
  width: 80%;
  position: absolute;
  top: 314rpx;
  left: 10%;
  font-size: 28rpx;
}
.outpop .pop_w .pop_title,
.lcPop .pop_w .pop_title {
  background: #47b34e;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
  line-height: 70rpx;
  text-align: center;
  color: #fff;
}
.outpop .pop_w .pop_con,
.lcPop .pop_w .pop_con {
  background: #fff;
  text-align: center;
  padding: 80rpx 40rpx;
  border-bottom-left-radius: 8px;
  border-bottom-right-radius: 8px;
}
.outpop .pop_w .pop_con text,
.lcPop .pop_w .pop_con text {
  display: block;
  line-height: 2;
}
.outpop .pop_w .pop_con text:first-of-type {
  text-align: left;
  text-indent: 20rpx;
}
.outpop .pop_w form button {
  opacity:0;
  width:25rpx;
  height:40rpx;
  position:absolute;
  top:13rpx;
  right:5rpx;
  z-index: 9999;
}
.icon_closew {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: 22rpx;
  right: 22rpx;
}

